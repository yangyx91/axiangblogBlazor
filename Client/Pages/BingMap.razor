@page "/loadmap"
@inject IJSRuntime Js

<div id="myMap" style='position:relative;width:600px;height:400px;'></div>
<button type="button" class="btn btn-primary" @onclick="LoadBingMap">
    LoadMap
</button>

@code {

    private void LoadBingMap()
    {
        var newJs = (IJSInProcessRuntime)Js;
        newJs.InvokeVoid("GetMap");
    }
}
